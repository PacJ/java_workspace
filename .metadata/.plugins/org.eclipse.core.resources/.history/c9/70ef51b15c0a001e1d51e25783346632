package problems0614;

abstract class Calc {
	double op1;
	double op2;
	
	void set(double op1, double op2) {
		this.set(op1, op2);
	}
	
	abstract double calculate();
}

class Add extends Calc {

	@Override
	double calculate() {
		return 0;
	}
	
}

class Sub extends Calc {

	@Override
	double calculate() {
		return 0;
	}
	
}

class Mul extends Calc {

	@Override
	double calculate() {
		return 0;
	}
	
}

class Div extends Calc {

	@Override
	double calculate() {
		return 0;
	}
	
}

