package Test;

class PhoneNum{
	String name;
	String phone;
	
	PhoneNum(String n, String p){
		name = n;
		phone = p;
	}
	
	public void show() {
		System.out.println("이름: " + name);
		System.out.println("번호 : " + phone);
	}
}

