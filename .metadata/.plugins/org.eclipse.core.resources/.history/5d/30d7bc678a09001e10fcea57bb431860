package Test;

import java.util.Scanner;

public class A0612_Scanner {
	
	static int big(int c, int d) {
		
		return c > d ? c : d;
	}
	
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int c=s.nextInt();
		int d=s.nextInt();
		
		System.out.println(big(c,d)); //입력한 두 수 중 큰값리턴(삼항 조건연산자)
	}

}
