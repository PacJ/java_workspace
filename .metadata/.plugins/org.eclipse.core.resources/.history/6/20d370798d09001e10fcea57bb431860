package Test;

class Profile {
	private int age;
	private String name;
	
	public Profile() {
		
	}
	
	public Profile(int age, String name) {
		this.age = age;
		this.name = name;
	}
	
	public void show() {
		System.out.println(age + " 살쳐먹고 뭐하는 짓이냐 " + name + "아(야)");
		
	}
}

public class A0612_Class {

	public static void main(String[] args) {
		
		Profile p1 = new Profile();
		
		p1.age=22;
		p1.name="원박";
		p1.show();
		
		Profile p2 = new Profile(39, "Keanu");
		p2.show();

	}

}
