package a0614;

import java.util.ArrayList;
import java.util.Scanner;

class Person {
	private String name;
	private int id;
	
	public Person() {

	}
	
	public Person(String name, int id) {
		this.name = name;
		this.id = id;
	}
	
	String getName() {
		return name;
	}
	
	int getId() {
		return id;
	}
	
	public boolean equals(Object obj) {
		
	}
}

class Game {
	Scanner s = new Scanner(System.in);
	
	ArrayList<Person> a = new ArrayList<Person>(5);
	
	public Game() { // 초기화 과정
		a.add(new Person("John", 14));
		a.add(new Person("Susan", 55));
		a.add(new Person("Edward", 23));
		a.add(new Person("Tyrone", 44));
		
	}
	
}
