package Test;

abstract class Car {
	String name; // 필드
	
	void run() {
		System.out.println(name+ " 차가 달린다.");
	}
	
	abstract void stop(); //추상 메소드(빈껍데기 함수);
}

class myCar extends Car {
	
	public myCar() {
		super();
	}

	@Override
	void stop() {
		System.out.println(name+"이 멈췄다");
	}
}

public class A0613_Abstract {

	public static void main(String[] args) {

	}

}
