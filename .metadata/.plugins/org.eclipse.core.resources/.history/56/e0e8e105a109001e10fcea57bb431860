package Problems;

//5. 0 이러한 모양 출력해라
//012
//01234
//0123456

public class Prob5 {

	public static void main(String[] args) {
		int[] arr = new int[3];
		int[] arr2 = new int[5];
		int[] arr3 = new int[7];
		
		for(int i = 0; i < arr.length; i++) {
			arr[i] = i;
			System.out.print(arr[i]);
		}
		
		for(int i = 0; i < arr2.length; i++) {
			arr[i] = i;
			System.out.print(arr[i]);
		}
		
		for(int i = 0; i < arr3.length; i++) {
			arr[i] = i;
			System.out.print(arr[i]);
		}

	}

}
