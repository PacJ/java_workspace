package problems0614_2;

//6. Main()을보고코드를작성해라.(c는 1~5까지 합, d는 1~10까지 합)

class add {
	int start;
	int finish;
	
	public add(int start, int finish) {
		this.start = start;
		this.finish = finish;
	}
}

public class problem6_2 {

	public static void main(String[] args) {

		int a[]={1,2,3,4,5};
		
		int b[]={6,7,8,9,10};
		
		int c=add(a,5);
		
		int d=add(a,5,b);
		
		System.out.println(c);
		
		System.out.println(d);

	}

}
