package Problems;
//4. main()를 보고 클래스 작성해라.

class Shape {
	int x;
	int y;
	
	public Shape() {

	}
	
	public Shape(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public void pr(String sym) {
		System.out.println(sym);
	}
}

public class Prob4 {

	public static void main(String[] args) {
		Shape s = new Shape();
		
		Shape s1 = new Shape(2,5);
		
		s.pr("@"); 
		
		s1.pr("#");
		
		s.show(); //@@@-> 1행 3열로 출력
		
		s.show(); //#####->2행 5열로 출력
		
		//#####


}
