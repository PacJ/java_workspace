package a0614;

import java.net.InetSocketAddress;
import java.net.Socket;

public class A0614_Client {

	public static void main(String[] args) {
		// 클라이언트단
		Socket so = null;

		try {
			so=new Socket();
			System.out.println("연결요청");
			
			so.connect(new InetSocketAddress("localhost", 5001));
		}
		
	}

}
