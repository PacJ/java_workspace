package Test;

class Profile {
	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	private int age;
	private String name;
	
	public Profile() {
		
	}
	
	public Profile(int age, String name) {
		this.age = age;
		this.name = name;
	}
	
	public void show() {
		System.out.println(age + " 살쳐먹고 뭐하는 짓이냐 " + name + "아(야)");
		
	}
}

public class A0612_Class {

	public static void main(String[] args) {
		
		Profile p1 = new Profile();
		
		p1.setAge(22);
		p1.setName("원박");
		p1.show();
		
		Profile p2 = new Profile(39, "Keanu");
		p2.show();

	}

}
